datasource db {
   provider = "postgresql"
   url      = env("DATABASE_URL")
}

generator client {
   provider = "prisma-client-js"
}

model Team {
   id         String  @id @unique @default(uuid())
   email      String  @unique
   full_name  String?
   avatar_url String?

   @@map(name: "team")
}

model Pokemon {
   id         String  @id @unique @default(uuid())
   email      String  @unique
   full_name  String?
   avatar_url String?

   @@map(name: "pokemon")
}
